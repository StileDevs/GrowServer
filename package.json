{
  "name": "growserver",
  "version": "2.4.0",
  "description": "a growtopia private server",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "npm run lint && swc ./src -d dist",
    "start": "node -r dotenv/config dist/src/app.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "lint": "eslint lint ./src",
    "iteminfo:fetch": "node wiki-scraper/scrape.js",
    "dev": "rimraf dist && rimraf assets/cache && eslint ./src && tsc && vite build --logLevel silent && node -r dotenv/config dist/app.js",
    "dev:nolint": "rimraf dist && rimraf assets/cache && tsc && node -r dotenv/config dist/src/app.js",
    "install": "(node scripts/setup.js) && npm run migrate && npm run seed && npm run finish",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "vite:dev": "vite",
    "finish": "echo \u001b[46mSetup Completed!\u001b[0m && echo \u001b[46mCheck out GrowServer github: https://github.com/JadlionHD/GrowServer\u001b[0m"
  },
  "author": "JadlionHD <jadlion.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/chance": "^1.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/decompress": "^4.2.7",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.13",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.19",
    "bits-ui": "^0.21.10",
    "clsx": "^2.1.1",
    "formsnap": "^1.0.0",
    "globals": "^15.4.0",
    "lucide-svelte": "^0.390.0",
    "mode-watcher": "^0.3.0",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.0",
    "svelte-sonner": "^0.3.24",
    "sveltekit-superforms": "^2.14.0",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0",
    "vaul-svelte": "^0.3.1",
    "vite": "^5.2.13",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "better-sqlite3": "^10.0.0",
    "chalk": "4.1.1",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "growtopia.js": "^1.4.1",
    "knex": "^3.1.0",
    "ws": "^8.17.0"
  }
}
